  "scripts": {
    "build": "nest build",
    "start": "npm run migration-prod:run && cross-env NODE_ENV=prod node dist/main",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration-dev:run": "cross-env NODE_ENV=dev npm run typeorm migration:run -- -d ./src/config/typeorm.ts",
    "migration-prod:run": "cross-env NODE_ENV=prod npm run typeorm migration:run -- -d ./dist/config/typeorm.ts",
    "migration:revert": "npm run typeorm -- -d ./src/config/typeorm.ts migration:revert",
    "migration:generate": "npm run typeorm -- -d ./src/config/typeorm.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "ts-node src/migrations/scripts/create-migration.script.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-yet": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "mysql2": "^3.10.2",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.7",
    "rotating-file-stream": "^3.2.3",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/rimraf": "^4.0.5",
    "@types/supertest": "^6.0.0",
